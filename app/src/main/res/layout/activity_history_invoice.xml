<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HistoryInvoiceActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/HistoryInvoiceActivity_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:fontFamily="serif"
                android:text="History"
                android:textColor="#333333"
                android:textSize="48sp"
                android:textStyle="bold" />

            <androidx.cardview.widget.CardView
                android:id="@+id/HistoryInvoiceActivity_invoiceInfo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="15dp"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="15dp">

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_invoiceInfoTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="serif"
                    android:paddingLeft="20dp"
                    android:paddingTop="15dp"
                    android:text="Invoice"
                    android:textColor="#333333"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity__roomName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="55dp"
                    android:text="Room Name"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity__roomCity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="85dp"
                    android:text="City"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_bedType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="115dp"
                    android:paddingBottom="15dp"
                    android:text="Bed Type"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_fromDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="145dp"
                    android:paddingBottom="15dp"
                    android:text="From"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_toDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="175dp"
                    android:paddingBottom="15dp"
                    android:text="To"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomPrice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="205dp"
                    android:paddingBottom="15dp"
                    android:text="Price"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomNameOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="55dp"
                    android:text="Room Name"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomCityOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="85dp"
                    android:text="City"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomBedTypeOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="115dp"
                    android:paddingBottom="15dp"
                    android:text="Bed Type"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_fromDateOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="145dp"
                    android:paddingBottom="15dp"
                    android:text="From"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_toDateOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="175dp"
                    android:paddingBottom="15dp"
                    android:text="To"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomPriceOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="205dp"
                    android:paddingBottom="15dp"
                    android:text="Price"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/HistoryInvoiceActivity_Price"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="15dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="15dp"
                app:cardCornerRadius="10dp">

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomDiscountOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="50dp"
                    android:text="Discount"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomDiscount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="50dp"
                    android:text="Discount"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomPriceXdurationOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="20dp"
                    android:text="Price"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_roomPriceXduration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="20dp"
                    android:text="Price"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_totalPriceOutput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="140dp"
                    android:layout_marginBottom="15dp"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="80dp"
                    android:text="Total Price"
                    android:textAlignment="textStart"
                    android:textColor="#333333"
                    android:textSize="20dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/HistoryInvoiceActivity_totalPrice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-light"
                    android:paddingLeft="20dp"
                    android:paddingTop="80dp"
                    android:text="Total Price"
                    android:textColor="#333333"
                    android:textSize="20dp"
                    android:textStyle="bold" />
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>